{
  "name": "sequelize-project-hetal",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "pre-commit": "eslint 'src/**/*.{js,ts}' --fix && prettier --write 'src/**/*.{js,ts,json,css,md}'",
    "sequelize": "ts-node -r tsconfig-paths/register ./node_modules/.bin/sequelize",
    "migrate:dev": "env-cmd -f .env.development npm run sequelize db:migrate",
    "start:dev": "env-cmd -f .env.development ts-node -r tsconfig-paths/register --transpile-only src/index.ts",
    "start-and-migrate:dev": "npm run start:dev && sleep 10 && npm run migrate:dev",
    "start:test": "env-cmd -f .env.test ts-node -r tsconfig-paths/register --transpile-only src/index.ts",
    "start:prod": "env-cmd -f .env.production ts-node -r tsconfig-paths/register --transpile-only src/index.ts",
    "migrate:test": "env-cmd -f .env.test sequelize-cli db:migrate",
    "revert:test:all": "env-cmd -f .env.test sequelize-cli db:migrate:undo:all",
    "migrate:prod": "env-cmd -f .env.production sequelize-cli db:migrate",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --write \"**/*.{ts,tsx,json,css,md}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.1",
    "sequelize": "^6.37.1",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.25",
    "@types/validator": "^13.11.9",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2"
  }
}
